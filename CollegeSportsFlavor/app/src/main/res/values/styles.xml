<?xml version="1.0" encoding="utf-8"?>
<resources>
	<style name="SideDrawerList">
        <item name="android:layout_gravity">start</item>
        <item name="android:background">#fff7f7f7</item>
        <item name="android:layout_width">240.0dip</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:drawSelectorOnTop">false</item>
        <item name="android:cacheColorHint">#fff7f7f7</item>
        <item name="android:divider">@null</item>
        <item name="android:choiceMode">singleChoice</item>
    </style>

    <style name="ScheduleTeamList">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_marginRight">10dp</item>
        <item name="android:textAppearance">?android:attr/textAppearanceListItem</item>
        <item name="android:textIsSelectable">true</item> 
    </style>

    <style name="SubTitleSmall">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_weight">1.0</item>
        <item name="android:layout_height">0dp</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:textAppearance">?android:textAppearanceSmall</item>
    </style>
</resources>
